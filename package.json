{
  "main": "./node_modules/expo/AppEntry.js",
  "description": "Mobile native app for Arthur Murray Franchisee Shop",
  "scripts": {
    "start": "expo start --lan -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx components/ containers/ actionCreators/ actionTypes/ reducers/ store/ utils/ navigation/ constants/ App.js bin/ __tests__/",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=19001'",
    "clean": "watchman watch-del-all && rm -rf package-lock.json && rm -rf node_modules && rm -rf ios/build/* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-* && npm install",
    "flow": "flow",
    "update-theme": "THEME_NAME=classic bash ./change-theme.sh"
  },
  "resolutions": {
    "deprecated-react-native-listview": "0.0.6"
  },
  "husky": {
    "hooks": {
      "_pre-push": "npm run lint && ./bin/no-push-master.sh"
    }
  },
  "jest": {
    "preset": "jest-expo"
  },
  "engines": {
    "node": "10.16.0",
    "npm": ">=6.9.0"
  },
  "dependencies": {
    "@expo/samples": "~3.0.3",
    "@expo/vector-icons": "^12.0.0",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "axios": "1.6.0",
    "date-fns": "2.10.0",
    "expo": "^40.0.0",
    "expo-app-loading": "1.0.1",
    "expo-asset": "~8.2.1",
    "expo-constants": "~9.3.3",
    "expo-font": "~8.4.0",
    "expo-web-browser": "~8.6.0",
    "lodash.clone": "4.5.0",
    "native-base": "2.13.4",
    "prop-types": "15.6.2",
    "query-string": "6.11.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-calendars": "1.217.0",
    "react-native-credit-card-input": "0.4.1",
    "react-native-datepicker": "1.7.2",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-lightbox": "0.8.0",
    "react-native-modal-dropdown": "0.6.2",
    "react-native-screens": "~2.15.2",
    "react-native-snap-carousel": "3.8.4",
    "react-native-status-bar-height": "2.3.1",
    "react-native-svg": "12.1.0",
    "react-native-ui-lib": "3.39.0",
    "react-native-vector-icons": "6.6.0",
    "react-native-web": "~0.13.12",
    "react-navigation": "3.12.0",
    "react-redux": "6.0.1",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "uuid": "3.3.3",
    "victory-native": "34.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-expo": "8.3.0",
    "eslint": "4.14.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-friendly-formatter": "2.0.7",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-chai-expect": "1.1.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.4.0",
    "flow-bin": "0.105.2",
    "husky": "3.0.1",
    "jest-expo": "^40.0.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6"
  },
  "author": "Marcus Hsu",
  "license": "UNLICENSED",
